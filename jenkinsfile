pipeline{
  agent {
    docker{ image 'node:latest'}
  }
    stages{
      stage('Build application'){
        steps{
        sh 'npm install'
        }
      }
                
      stage('Test') {
        steps {
          sh 'npm test'
        }
      }
                
      stage('Deploy') {
        steps {
          sh 'npm start'
        }
      }          
  }
}
